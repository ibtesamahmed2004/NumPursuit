
import random
n = random.randint(1, 100)

a = -1

guesses = 0

while(a != n):
    guesses += 1
    a = int(input("Guess the Number: "))
    
    if(a > n):
        print("Lower the Number")
        
    else:
        print("Higher the Number")
        
print(f"You have Guessed the number {n} in {guesses} attempts")